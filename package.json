{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "Gulp template for page-proofs",
  "main": "gulpfile.js",
  "scripts": {
    "test": "export ENV=testing; ./node_modules/mocha/bin/mocha --timeout 15000 --recursive tests/tests",
    "cover": "export ENV=testing; istanbul cover --report clover _mocha tests/tests/**/**",
    "coveralls": "export ENV=testing; npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "generate-html": "node ./node_modules/jsdoc/jsdoc.js -t template -c jsdoc.conf.json -d",
    "install": "echo '' > .pids",
    "clear-out": "$(rm -rf `find tests/data/. -type f ! -iname \".gitignore\" | grep \"/out/\"`) && $(rm -rf `find tests/test-project/build/. -type f ! -iname \".gitignore\"`)"
  },
  "repository": {
    "url": "https://github.com/sonrac/gulp-template",
    "type": "git"
  },
  "licenses": [
    "ICS"
  ],
  "author": "Donii Sergii <doniysa@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "jpegtran": "^1.0.5",
    "optipng": "^1.1.0",
    "typescript": "^3.1.5"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1",
    "@babel/plugin-transform-typescript": "^7.1",
    "@babel/preset-env": "^7.1",
    "babel-core": "7.0.0-bridge.0",
    "backgrounder": "^0.2.7",
    "chai": "^4.0.2",
    "chai-fs": "^2.0",
    "coveralls": "^3.0",
    "eslint": "^5.8",
    "fs": "0.0.1-security",
    "gulp": "^4.0",
    "gulp-autoprefixer": "^6.0",
    "gulp-babel": "^8.0",
    "gulp-clean-css": "^3.9",
    "gulp-coffee": "^2.0",
    "gulp-concat": "^2.6",
    "gulp-connect": "^5.6",
    "gulp-htmlmin": "^5.0",
    "gulp-imagemin": "^4.0",
    "gulp-less": "^4.0",
    "gulp-livereload": "^4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-pug": "^4.0",
    "gulp-refresh": "^1.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-requirejs": "^1.1.1",
