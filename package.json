{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "Gulp template for page-proofs",
  "main": "gulpfile.js",
  "scripts": {
    "test": "export ENV=testing; ./node_modules/mocha/bin/mocha --timeout 15000 --recursive tests/tests",
    "cover": "export ENV=testing; istanbul cover --report clover _mocha tests/tests/**/**",
    "coveralls": "export ENV=testing; npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "generate-html": "node ./node_modules/jsdoc/jsdoc.js -t template -c jsdoc.conf.json -d",
    "install": "echo '' > .pids"
  },
  "repository": {
    "url": "https://github.com/sonrac/gulp-template",
    "type": "git"
  },
  "licenses": [
    "ICS"
  ],
  "author": "Donii Sergii <doniysa@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "gulp-sass": "^3.1.0",
    "sass": "^0.5.0"
  },
  "optionalDependencies": {
    "gulp-coffee": "^2.3.4",
    "gulp-sass": "^3.1.0",
    "gulp-stylus": "^2.6.0",
    "gulp-typescript": "^3.1.7",
    "gulp-typescript-babel": "^1.0.0",
    "jsdoc": "^3.4.3"
  },
  "devDependencies": {
